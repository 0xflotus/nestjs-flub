{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-unused-expression": false,
        "interface-name": false,
        "no-console": false,
        "no-bitwise": false,
        "member-access": false
    },
    "rulesDirectory": []
}